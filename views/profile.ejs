    <!-- START: Body Content -->
  <div class="body-container">
  <nav class="sidebar-container overflow-y-auto">
    <ul class="sidebar-tech-links">
      <a href="/"><li><span><i class="fa-solid fa-house"></i></span><span>Home</span></li></a>
      <a href="/profile"><li><span><i class="fa-solid fa-user"></i></span><span>Profile</span></li></a>
      <hr>
      <span class="sidebar-header">MY TECH</span>
      <% for (let i = 0; i < fields.length; i++) {%>
        <a href="profile/<%= fields[i].techName %>"><li><span><%= fields[i].techName %></span></li></a>
      <% } %>
    </ul>
  </nav>
  <main class="main overflow-y-auto">
      <section class="ezy__header30_amwY6uCW">
    <div class="ezy__header30_amwY6uCW-topbar"
      style="background-image: url(images/profile-header-bg.jpg)">
      <div class="ezy__header30_amwY6uCW-overlay"></div>
      <div class="container">
        <div class="row justify-content-center text-center">
          <div class="col-12">
            <h1 class="ezy__header30_amwY6uCW-heading mb-4">Track Your Web Development Journey</h1>
            <div class="row justify-content-center">
              <div class="col-12 col-lg-5 text-center">
                <p class="ezy__header30_amwY6uCW-sub-heading">
                  Search for technologies, add them to your stack, and start tracking your spaced repetition sessions.
                </p>
                <form class="ezy__header30_amwY6uCW-search-form" id="techInputForm" action="#">
                  <div class="input-group mt-4">
                    <input class="form-control" name="techInput" id="techInput" type="text" placeholder="Add Technologies" aria-label="Search" autocomplete="off"/>
                    <button class="btn ezy__header30_amwY6uCW-btn" id="addTech" type="submit" value="addTech"><i class="fas fa-add"></i></button>
                    <ul id="suggestions" class="suggestions-container">
                    </ul>
                  </div>
                </form>
                
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    
  </section>

  <section class="ezy__portfolio12 gray">
    <div class="container">
      <div class="row justify-content-center">
        <h1 class="ezy__portfolio12-heading text-center">My Technology Stack</h1>

