                    src="https://cdn.easyfrontend.com/pictures/courses/courses1.jpg"
                    class="card-img-top"
                    alt="..."
                  />
                  <!-- Add class="card-img-top" after src in case you need it-->
                </div> 
                <div class="card-body">
                  <div class="close">
                      
                  </div>
                  <a>
                    <p class="card-text mb-2"><%= fields[i].category %></p>
                  </a>
                  <a href="profile/<%= fields[i].techName %>" target="_blank">
                    <h5 class="card-title mb-2"><%= fields[i].techName %></h5>
                  </a>
                  <p class="card-text">Some quick example text to build on the card the bulk content...</p>
                  <div class="d-flex justify-content-between mb-2">
                    <div class="d-flex align-items-center">
                      <div class="me-2">
                        <img
                          src="https://cdn.easyfrontend.com/pictures/testimonial/testimonial_square_1.jpeg"
                          alt=""
                          class="img-fluid rounded-circle border"
                          width="40"
                        />
                      </div>
                      <div>
                        <h4 class="mb-0">Progress: <%= fields[i].progress %> </h4>
                      </div>
                    </div>
                    <a href="profile/<%= fields[i].techName %>" target="_blank" class="btn btn-outline ezy__portfolio12-card-btn" type="button">Track</a>
                    
                  </div>
                </div>
              </div>
            </div>
            <% } %>
            <!-- card ends -->
          </div>
        </div>
